{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@Components/*": [
        "app/components/*"
      ],
      "@Styles/*": [
        "app/styles/*"
      ],
      "@Hooks/*": [
        "app/hooks/*"
      ],
      "@Types/*": [
        "app/types/*"
      ],
    }
  }
}